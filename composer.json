{
    "name": "joomla/joomla-cms",
    "type": "project",
    "description": "Joomla CMS",
    "keywords": ["joomla", "cms"],
    "homepage": "https://github.com/joomla/joomla-cms",
    "license": "GPL-2.0+",
    "config": {
        "vendor-dir": "libraries/vendor"
    },
    "require": {
        "php": ">=5.5",
        "joomla/application": "~1.5",
        "joomla/di": "~1.2",
        "joomla/event": "~1.1",
        "joomla/registry": "~1.4 >=1.4.5",
        "joomla/session": "~1.3",
        "joomla/string": "~1.3",
        "joomla/uri": "~1.1",
        "joomla/utilities": "~1.3 >=1.3.3",
        "ircmaxell/password-compat": "1.*",
        "leafo/lessphp": "0.3.9",
        "phpmailer/phpmailer": "5.2.9",
        "symfony/yaml": "2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4",
        "phpunit/dbunit": "~1.3",
        "squizlabs/php_codesniffer": "~1.5",
        "pear/cache_lite": "1.7.16",
        "codeception/codeception": "^2.1",
        "joomla-projects/joomla-browser": "^1.2"
    },
    "autoload-dev": {
        "psr-4": {
            "Joomla\\Tests\\Unit\\": "tests/unit/",
            "Joomla\\Tests\\System\\": "tests/acceptance/",
            "Joomla\\Tests\\Page\\": "tests/_support/Page/"
        }
    }
}
